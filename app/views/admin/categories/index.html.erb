<h1>Category</h1>
<thead>
<td>Name</td>
<td>Action</td>
</thead>
<ul>
  <% @categories.each do |category| %>
    <li>
      <%= category.name %>
      <td>
        <%= link_to :edit, edit_category_path(category) %>
        <%= button_to :delete, category_path(category), method: :delete %>
      </td>
    </li>
  <% end %>
</ul>
